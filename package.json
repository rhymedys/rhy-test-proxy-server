{
  "name": "test_proxy_server",
  "version": "1.0.0",
  "description": "一个方便前端本地测试不同环境的api代理服务器，有助于本地前端代码处于开发模式下与生产环境接口联调。",
  "private": true,
  "dependencies": {
    "egg": "^2.22.2",
    "egg-cors": "^2.2.0",
    "egg-mongoose": "^3.1.1",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.11.0",
    "egg-view-ejs": "^2.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "moment": "^2.29.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.13.1",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.22.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "ENABLE_NODE_LOG=YES egg-scripts start --port=7002 --daemon --title=test_proxy_server",
    "stop": "egg-scripts stop --title=test_proxy_server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "rhymedys<rhymedys@gmail.com>",
  "license": "MIT"
}
